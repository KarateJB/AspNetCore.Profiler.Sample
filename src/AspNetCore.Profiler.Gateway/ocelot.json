{
  "Routes": [
    {
      "UpstreamPathTemplate": "/Payment/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "DownstreamPathTemplate": "/api/PaymentApi/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "QoSOptions": {
        "TimeoutValue": 5000
      },
      "UpstreamHeaderTransform": {
        "Referrer": "{RemoteIpAddress}",
        "X-Forwarded-For": "{RemoteIpAddress}"
      },
      "FileCacheOptions": {
        "TtlSeconds": 3600,
        "Region": "HelloWorld",
        "EnableContentHashing": false
      }
    },
    {
      "UpstreamPathTemplate": "/Demo/{everything}",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "DownstreamPathTemplate": "/api/DemoApi/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "QoSOptions": {
        "TimeoutValue": 3000
      },
      "UpstreamHeaderTransform": {
        "Referrer": "{RemoteIpAddress}",
        "X-Forwarded-For": "{RemoteIpAddress}"
      }
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId"
  }
}
